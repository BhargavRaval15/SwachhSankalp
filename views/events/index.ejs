<%- include('../partials/header') %>

<h2>Events</h2>

<a href="/events/new">Create New Event</a>

<ul>
  <% events.forEach(function(event) { %>
  <li>
    <a href="/events/<%= event._id %>"><%= event.title %></a>
    <p><%= event.description %></p>
  </li>
  <% }) %>
</ul>

